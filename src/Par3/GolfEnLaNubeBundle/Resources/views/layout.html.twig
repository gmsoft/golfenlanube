<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
	    <meta name="viewport" content="width=device-width, initial-scale=1.0">

	    <title>{% block title %}Bienvenido!{% endblock %}</title>

	    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
		
		<!-- Bootstrap -->
	    <link href="{{ asset('/bundles/golfenlanube/css/bootstrap.css') }}" rel="stylesheet" media="screen" />
	    <!-- link href="{#{ asset('/bundles/golfenlanube/css/menu.css') }#}" rel="stylesheet" media="screen" -->

	    <!-- link rel="stylesheet" href="//fonts.googleapis.com/css?family=Lato:400,700,300,600" type="text/css" />
  		<link rel="stylesheet" href="//fonts.googleapis.com/css?family=Comfortaa:400,700,300" type="text/css" / -->
	    <link href="{{ asset('/bundles/golfenlanube/css/ajustes.css') }}" rel="stylesheet" media="screen"/>

		{% block stylesheets %}{% endblock %}
		<!-- HTML5 Shim and Respond.js add IE8 support of HTML5 elements and media queries -->
	    {% include 'BraincraftedBootstrapBundle::ie8-support.html.twig' %}
	</head>
	<body>
		<nav class="navbar navbar-default navbar-fixed-top" role="navigation" id="menuUsuario">
		    <div class='container'>
				<!-- Brand and toggle get grouped for better mobile display -->
			    <div class="navbar-header">
			        <button type="button" class="navbar-toggle"
			                data-toggle="collapse" data-target=".navbar-ex1-collapse">
			            <span class="sr-only">Toggle navigation</span>
			            <span class="icon-bar"></span>
			            <span class="icon-bar"></span>
			            <span class="icon-bar"></span>
			        </button>
			        <!-- a class="navbar-brand" href="#">Inicio</a -->
			        <!-- a class="navbar-brand" href="#"><img width="101" height="87" src="{{ asset('bundles/golfenlanube/images/logo-web.png') }} "></a -->
			    </div>
	
			    <!-- Collect the nav links, forms, and other content for toggling -->
			    <div class="collapse navbar-collapse navbar-ex1-collapse">
			        {{ knp_menu_render('main', { 'style': 'navbar' }) }}
				    <ul class="nav navbar-nav navbar-right">
				    	<li>
				    		<a href="{{ path('fos_user_security_logout') }}">Salir</a>
				    	</li>
				    </ul>
			        
			    </div><!-- /.navbar-collapse -->
		    </div>
		</nav>

		<div class='container' style='padding-bottom: 70px;'>
			<div class='row'>
				<div class='col-lg-12'>
					
					<div class='row '>
						<div class='col-md-12 '>
							<div class="page-header">
								{% block page_header %}{% endblock %}
							</div>
						</div>
					</div>
					
					<div class='row'>
						<div class='col-md-12'>
						{% include 'BraincraftedBootstrapBundle::flash.html.twig' with {'close' : true } %}
						</div>
					</div>

        			{% block body %}{% endblock %}
        		</div>
        		<!-- div id='publicidad' class='col-lg-4'>
        			{#% block publicidad %#}&nbsp; {#% endblock %#}
        		</div-->
        	</div>
        </div>
        
		<script src="{{ asset('/bundles/golfenlanube/js/jquery-1.10.2.js') }}" ></script>
		<script src="{{ asset('/bundles/golfenlanube/js/bootstrap.js') }}" ></script>
		<script src="{{ asset('/bundles/golfenlanube/js/general.js') }}" ></script>
		
		{% block javascripts %}{% endblock %}
		
    </body>
</html>
